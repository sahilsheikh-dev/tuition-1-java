package com.firstproject.conditional;

public class SwitchCase {
	
	public static void main(String[] args) {
		
		int number = 10;
		
		switch (number) {
		case 0:
			System.out.println("Number is 0");
			break;
		case 10:
			System.out.println("Number is 0");
			break;
		case 20:
			System.out.println("Number is 0");
			break;
		case 30:
			System.out.println("Number is 0");
			break;
		case 40:
			System.out.println("Number is 0");
			break;
			
		}
		
	}

}
