package com.firstproject.conditional;



public class IfElse {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);

		int number = 1000;

		if (number > 100) {
			System.out.println("Number is greater that 100");
		} else {
			System.out.println("Number is less then 100");
		}

	}

}
