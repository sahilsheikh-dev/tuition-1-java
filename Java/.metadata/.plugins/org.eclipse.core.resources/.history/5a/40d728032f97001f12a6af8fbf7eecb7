package com.firstproject.methods;

public class TypesOfMethods {
	
	public static void main(String[] args) {
		
		/*
		 * instance method
		 * static method
		 * */
		
		TypesOfMethods obj1 = new TypesOfMethods();
		obj1.greeting1();
		
	}
	
	public int greeting1() {
		return 0;
	}
	
	public String greeting2() {
		return "Abc";
	}
	
	public void greeting3() {
//		no need to return anything in void return type
	}

}
